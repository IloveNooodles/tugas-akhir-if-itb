{
  "@context": ["http://w3c.github.io/bigiot/bigiot- recipe-model.jsonld"],
  "name": "Route to nearest parking spot",
  "@type": "recipe",
  "category": "parking",
  "recipeId": "http://marketplace/bigiot/RouteToParkingSpot",
  "description": "A recipe to retrieve directions to the nearest parking spot via email ",
  "hasIngredients": [
    {
      "name": "Get parking spots",
      "@type": "Offering",
      "offeringCategory": "schema:parking",
      "inputData": [
        {
          "name": "userLocation",
          "rdf:type": "schema:from"
        }
      ],
      "outputData": [
        { "name": "parkingCoordinates", "rdf:type": "schema: GeoCoordinates" }
      ],
      "name-dir": "Get directions",
      "inputData": [
        { "name": "userDestination", "rdf:type": "schema:to" },
        { "name": "userLocation", "rdf:type": "schema:from" }
      ],
      "outputData": [{ "name": "directions", "rdf:type": "schema:Map" }],
      "hasInteraction": [
        {
          "@id": "DirectionsToParking",
          "fromIngredient": { "@id": "Get parking spots" },
          "hasInteractionOutput": { "@id": "parkingCoordinates" },
          "toIngredient": { "@id": "Get directions" },
          "hasInteractionInput": { "@id": "userDestination" }
        }
      ]
    }
  ]
}
